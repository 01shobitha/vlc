msgid "fast"
msgstr "hitro"

#: modules/codec/x264.c:354
msgid "normal"
msgstr "normalno"

#: modules/codec/x264.c:354
msgid "slow"
msgstr "počasi"

#: modules/codec/x264.c:354
msgid "all"
msgstr "vse"

#: modules/codec/x264.c:360 modules/codec/x264.c:365
msgid "spatial"
msgstr "prostorsko"

#: modules/codec/x264.c:360 modules/codec/x264.c:365
msgid "temporal"
msgstr "začasno"

#: modules/codec/x264.c:360 modules/gui/pda/pda_interface.c:743
