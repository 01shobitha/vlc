#: src/libvlc-module.c:542 modules/gui/macosx/equalizer.m:143
#: modules/gui/macosx/extended.m:102 modules/gui/macosx/wizard.m:394
#: modules/gui/qt4/ui/equalizer.ui:36 modules/gui/qt4/ui/vlm.ui:67
