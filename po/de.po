"  [vcd:][Gerät][@[Titel][,[Kapitel]]\n"
"                                 VCD Gerät\n"
"  udpstream:[@[<bind Adresse>][:<bind port>]]\n"
"                                 UDP stream gesendet von VLS\n"
"  vlc:loop                       Abspielen der Abspielliste endlos "
"wiederholen\n"
"  vlc:pause                      Abspielen von Abspiellistenelemten "
"pausieren\n"
"  vlc:quit                       \tVLC beenden"

